<!--pages/student/student.wxml-->
<view class='container'>
  <form bindsubmit='submit'>
    <view class='row'>
      <text>学号：</text>
      <input type='text' name="studentId" placeholder='请输入学号' value='{{studentId}}'></input>
    </view>
    <view class='row'>
     <text>密码：</text>
     <input type='password' name='password' placeholder='请输入密码' value='{{password}}'></input>
    </view>
    <view>
     <button form-type='submit' type='primary'>导入</button>
    </view>
  </form>
</view>
